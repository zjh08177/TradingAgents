# Production Environment Variables for LangGraph Cloud
# Copy this file to .env and fill in your actual API keys

# Required: LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here  # Optional
# GOOGLE_API_KEY=your_google_api_key_here  # Optional

# Required: Data Sources
SERPER_API_KEY=your_serper_api_key_here
FINNHUB_API_KEY=your_finnhub_api_key_here

# Optional: Trading Agent Configuration
TRADINGAGENTS_RESULTS_DIR=/app/results
TRADINGAGENTS_DATA_DIR=/app/results/data
MAX_DEBATE_ROUNDS=1
MAX_RISK_DISCUSS_ROUNDS=1
MAX_RECUR_LIMIT=50

# Optional: Performance Tuning
BG_JOB_TIMEOUT_SECS=3600

# Optional: LangSmith Tracing (automatically configured in LangGraph Cloud)
# LANGSMITH_API_KEY=your_langsmith_api_key_here
# LANGSMITH_PROJECT=trading-agents-production 